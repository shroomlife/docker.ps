{
  "name": "docker.ps",
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "dev": "nuxt dev",
    "docker:build": "docker build -t shroomlife/docker.ps:latest .",
    "docker:down": "docker compose down",
    "docker:publish": "bun run docker:build && bun run docker:push",
    "docker:push": "docker push shroomlife/docker.ps:latest",
    "docker:up": "docker compose up -d",
    "generate": "nuxt generate",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "preview": "nuxt preview",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@prisma/client": "6.16.1",
    "axios": "1.12.2",
    "dockerode": "4.0.8",
    "jsonwebtoken": "9.0.2",
    "moment": "2.30.1"
  },
  "devDependencies": {
    "@nuxt/eslint": "1.9.0",
    "@nuxt/ui": "4.0.0-alpha.1",
    "@pinia/nuxt": "0.11.2",
    "@types/dockerode": "3.3.43",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.3.1",
    "eslint": "9.35.0",
    "nuxt": "^4.1.2",
    "pinia": "3.0.3",
    "prisma": "6.16.1",
    "typescript": "5.9.2",
    "vue": "3.5.21",
    "vue-router": "4.5.1",
    "vue-tsc": "3.0.7"
  },
  "packageManager": "bun@1.2.21",
  "overrides": {
    "vite": "npm:rolldown-vite@latest"
  }
}
